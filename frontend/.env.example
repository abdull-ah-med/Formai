# Frontend environment variables
# Copy to frontend/.env and fill in values

# Point to backend API base (Vite dev server proxies /api to localhost:4000 by default)
VITE_API_BASE_URL=http://localhost:4000/api

# Public site URL used to construct Google redirect URI
VITE_FRONTEND_URL=http://localhost:5173

# Google OAuth Client ID (from Google Cloud console)
VITE_GOOGLE_CLIENT_ID=

# reCAPTCHA site key (client-side)
VITE_RECAPTCHA_SITE_KEY=
