# Backend environment variables
# Copy to backend/.env and fill in values

PORT=4000
MONGODB_URI=mongodb://localhost:27017/formai
JWT_SECRET=replace_with_strong_secret
FRONTEND_URL=http://localhost:5173

# Anthropic Claude
ANTHROPIC_API_KEY=

# Google OAuth (Web application)
GOOGLE_CLIENT_ID=
GOOGLE_CLIENT_SECRET=

# reCAPTCHA (server-side secret)
RECAPTCHA_SECRET_KEY=

# Must be exactly 32 characters for AES-256 encryption
ENCRYPTION_KEY=32_character_secret_key_here_1234

# NODE_ENV can be development or production
NODE_ENV=development
